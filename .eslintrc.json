{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-undef": 0,
        "class-methods-use-this": 0,
        "consistent-return": 0,
        "no-prototype-builtins": 0,
        "no-return-assign": 0,
        "eqeqeq": 0,
        "no-underscore-dangle": 0,
        "no-console": 0,
        "no-restricted-globals": 0,
        "no-unused-vars": 0,
        "allowTemplateLiterals": 0
    }
}
